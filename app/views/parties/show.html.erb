<%= form_for [@party, @party.votes.new] do |f| %>
  <div class="form-group">
    <%= f.select :points, [0, 1, 2, 3, 5, 8], {}, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.submit 'Vote!', class: 'btn btn-success' %>
    <%= link_to("Watch",watch_party_path(@party)) %>
  </div>
<% end %>
